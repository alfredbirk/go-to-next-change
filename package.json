{
    "name": "go-to-next-change",
    "displayName": "go-to-next-change",
    "description": "",
    "version": "0.0.1",
    "icon": "src/logo.png",
    "engines": {
        "vscode": "^1.83.0"
    },
    "keywords": [
        "next",
        "change",
        "changed",
        "file",
        "files",
        "shortcut",
        "hotkey",
        "git",
        "scm",
        "source control",
        "cycle",
        "previous"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "go-to-next-change.go-to-next-scm-change",
                "title": "Go to next git change"
            },
            {
                "command": "go-to-next-change.go-to-previous-scm-change",
                "title": "Go to previous git change"
            }
        ]
    },
    "keybindings": [
        {
            "command": "go-to-next-change.go-to-next-scm-change",
            "key": "alt+l",
            "mac": "alt+l"
        },
        {
            "command": "go-to-next-change.go-to-previous-scm-change",
            "key": "alt+k",
            "mac": "alt+k"
        }
    ],
    "scripts": {
        "vscode:prepublish": "pnpm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.83.0",
        "@types/mocha": "^10.0.3",
        "@types/node": "18.x",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "typescript": "^5.2.2",
        "ts-loader": "^9.5.0",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "@vscode/test-electron": "^2.3.6"
    }
}
